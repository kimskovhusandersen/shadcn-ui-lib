{
  "name": "@kimskovhusandersen/ui-library",
  "version": "1.0.28",
  "private": false,
  "description": "An interface library crafted for React, integrating Tailwind CSS, Vite, TypeScript, and Radix UI and Shadcn UI components.",
  "homepage": "https://github.com/kimskovhusandersen/shadcn-ui-lib#readme",
  "bugs": {
    "url": "https://github.com/kimskovhusandersen/shadcn-ui-lib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimskovhusandersen/shadcn-ui-lib.git"
  },
  "license": "MIT",
  "author": "Kim Skovhus Andersen",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc && vite build && npm run build:styles",
    "build-storybook": "storybook build",
    "build:styles": "postcss ./src/index.css -o ./dist/style.css",
    "dev": "vite",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepublish": "npm test && npm run build && npm version patch -m 'Bump to %s'",
    "storybook": "storybook dev -p 6006",
    "test": ""
  },
